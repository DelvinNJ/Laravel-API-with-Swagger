                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "products",
                        "in": "query",
                        "description": "The number of products to return per page, the default count is 50",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "true"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
